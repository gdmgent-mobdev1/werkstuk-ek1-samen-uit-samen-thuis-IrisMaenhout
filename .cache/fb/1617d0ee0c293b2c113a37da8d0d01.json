\r\n    const yesBtn = popup.querySelector('.yes');\r\n    const noBtn = popup.querySelector('.no');\r\n\r\n\r\n    noBtn.addEventListener('click', () => {\r\n        popup.remove();\r\n        overlay.remove();\r\n    });\r\n\r\n    yesBtn.addEventListener('click', async () => {\r\n\r\n\r\n        const userRef = collection(db, \"users\");\r\n\r\n        const q = query(userRef, where(\"userId\", \"==\", user.uid), );\r\n\r\n        const querySnapshot = await getDocs(q);\r\n        querySnapshot.forEach(async (docUser) => {\r\n            console.log(docUser.id, \" => \", docUser.data());\r\n\r\n            await deleteDoc(doc(db, \"users\", docUser.id));\r\n        });\r\n\r\n\r\n\r\n        deleteUser(user).then(() => {\r\n            console.log('user is removed')\r\n        }).catch((error) => {\r\n            console.log(error);\r\n        });\r\n    });\r\n\r\n}\r\n\r\nconst firebaseAppConfig = getFirebaseConfig();\r\ninitializeApp(firebaseAppConfig);\r\nexport default removeUser;"},"lineCount":null}},"error":null,"hash":"323e14473090a3605d816aa609b3f325","cacheData":{"env":{}}}